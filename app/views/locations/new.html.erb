<h2>Create Location</h2>
<%= form_for [@trip, @location] do |f| %>

<%= f.label :city %>
<%= f.text_field :city, required: true %>

<%= f.label :state %>
<%= f.text_field :state, required: true %>

<%= f.label :country %>
<%= f.text_field :country, required: true %>

<%= f.submit %>
</div>

<% end %>
